{
  "name": "mon-toit-platform",
  "private": true,
  "version": "3.2.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build --config vite.config.ts",
    "build:standard": "vite build",
    "build:ultra": "vite build --config vite.config.ultra-optimized.ts",
    "build:analyze": "vite-bundle-analyzer dist",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit -p tsconfig.app.json",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "prepare": "husky install"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@react-email/components": "^1.0.1",
    "@react-email/render": "^2.0.0",
    "@sentry/browser": "^10.29.0",
    "@sentry/react": "^10.26.0",
    "@sentry/replay": "^7.116.0",
    "@sentry/tracing": "^7.120.4",
    "@sentry/vite-plugin": "^4.6.1",
    "@supabase/supabase-js": "^2.57.4",
    "@tanstack/react-query": "^5.90.5",
    "@types/dompurify": "^3.0.5",
    "@types/mapbox-gl": "^3.4.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dompurify": "^3.3.0",
    "jspdf": "^3.0.3",
    "lucide-react": "^0.344.0",
    "mapbox-gl": "^3.16.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-email": "^5.0.5",
    "react-ga4": "^2.1.0",
    "react-helmet-async": "^2.0.5",
    "react-hook-form": "^7.68.0",
    "react-router-dom": "^6.30.1",
    "tailwind-merge": "^3.4.0",
    "zod": "^4.1.13",
    "zustand": "^4.5.7"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^14.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@types/sharp": "^0.31.1",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/coverage-v8": "^1.6.1",
    "@vitest/ui": "^1.6.1",
    "autoprefixer": "^10.4.18",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "husky": "^8.0.3",
    "jsdom": "^23.2.0",
    "lint-staged": "^15.5.2",
    "postcss": "^8.4.35",
    "prettier": "^3.6.2",
    "rollup-plugin-visualizer": "^6.0.5",
    "sharp": "^0.34.5",
    "tailwindcss": "^3.4.1",
    "terser": "^5.44.1",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.21",
    "vite-bundle-analyzer": "^1.2.4",
    "vitest": "^1.6.1"
  }
}
